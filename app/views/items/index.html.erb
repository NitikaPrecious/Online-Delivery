
<h1>All Items</h1>

<table>
<thead>
    <tr>
      <th>Item Name</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.description%></td>
        <td><%= item.price%></td>
        <td><%= link_to "Show", item %>  </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>